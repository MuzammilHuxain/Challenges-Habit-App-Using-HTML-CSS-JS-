          <div class="guidingSpanDivision">
            <div class="spansDivision">
              <div class="spanDiv" id="allCompleteDiv">
                <span class="guideColor" id="allCompleteGuideColor" ></span>
                <span id="guideText">All Complete</span>
              </div>
              <div class="spanDiv" id="notCompleteDiv">
                <span class="guideColor"></span>
                <span id="guideText">Not Complete</span>
              </div>
            
            </div>
            <div class="spanDiv" id="inProgressDiv">
              <span class="guideColor"></span>
              <span id="guideText">In Progress</span>
            </div>
            
            
          </div>


            <button class="primary-Buttons">Continue</button>
        </div>





      </div>

     

    </header>
   
   
    <script src="../Javascript Files/index.js"></script>
